\newcommand{\graphic}[2]{
	\immediate\write18{curl "***"  --data-urlencode "x=#1" > '#2'.png}
}

\newcommand{\calc}[1]{
	\immediate\write18{rm -f calc.alphatex}
	\immediate\write18{curl "***" --data-urlencode "x=#1" > calc.alphatex}
	\input{calc.alphatex}
	\immediate\write18{rm -f calc.alphatex}
}

\newcommand{\WolframAlpha}[1]{
	\immediate\write18{rm -f wolfalph.alphatex}
	\immediate\write18{curl "***" --data-urlencode "x=#1" > wolfalph.alphatex}
	\input{wolfalph.alphatex}
	\immediate\write18{rm -f wolfalph.alphatex}
}

